<Application x:Class="MonopolyDesign.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" StartupUri="MainWindow.xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" d1p1:Ignorable="d" xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:converters="clr-namespace:MonopolyDesign.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <vm:ViewModelLocator x:Key="Locator" d:IsDataSource="True" xmlns:vm="clr-namespace:MonopolyDesign.ViewModel" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
					<converters:IntToString x:Key="IntToString" />
					<converters:StringToUpper x:Key="StringToUpper" />
				</ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
	        
        </ResourceDictionary>

    </Application.Resources>
</Application>